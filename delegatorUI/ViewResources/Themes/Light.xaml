<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="Text" Color="Black"/>
    <Color x:Key="TextClr" R="#0" G="#0" B="#0"/>

    <SolidColorBrush x:Key="Text2" Color="#FF8D8D8D"/>

    <SolidColorBrush x:Key="WindowPanel" Color="#eeeeee"/>

    <SolidColorBrush x:Key="MainBg" Color="#e2e4de"/>

    <SolidColorBrush x:Key="NavPanel" Color="#cccccc"/>

    <SolidColorBrush x:Key="Hover" Color="#dddddd"/>

    <SolidColorBrush x:Key="Path" Color="#111111"/>
    
    <SolidColorBrush x:Key="TaskBg" Color="#555555"/>

    <SolidColorBrush x:Key="PanelButtonChecked" Color="White"/>
